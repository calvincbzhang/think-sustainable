<section class="tsd-panel tsd-member {{cssClasses}}" {{#if signatures}}{{#each signatures}}{{#if this.comment.tags}}data-tags="{{#each this.comment.tags}}{{tagName}} {{/each}}"{{/if}}{{/each}}{{/if}}>
    <a name="{{anchor}}" class="tsd-anchor"></a>
    {{#if name}}
        <h3>{{#each flags}}<span class="tsd-flag ts-flag{{this}}">{{this}}</span> {{/each}}{{{wbr name}}}</h3>
    {{/if}}

    {{#if signatures}}
        {{> member.signatures}}
    {{else}}{{#if hasGetterOrSetter}}
        {{> member.getterSetter}}
    {{else}}
        {{> member.declaration}}
    {{/if}}{{/if}}

    {{#each groups}}
        {{#each children}}
            {{#unless hasOwnDocument}}
                {{> member}}
            {{/unless}}
        {{/each}}
    {{/each}}
</section>
